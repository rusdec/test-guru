<h1><%= @test_passage.test.title %> <%= t('.completed') %></h1>
<p><%= @test_passage.correct_answers_percent %>% <%= t('.out_of') %></p>
<% if @test_passage.correct_answers_percent >= 85 %>
  <p class="passed"><%= t('.passed') %></p>
<% else %>
  <p class="failed"><%= t('.failed') %></p>
<% end %>
<%= link_to t('common.tests'), tests_path %>